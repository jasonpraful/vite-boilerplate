{
	"extends": "./tsconfig.browser.json",
	"compilerOptions": {
		"composite": true,
		"moduleResolution": "Bundler",
		"module": "ESNext",
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"],
			"@components/*": ["src/components/*"],
			"@hooks/*": ["src/hooks/*"],
			"@pages/*": ["src/pages/*"],
			"@styles/*": ["src/styles/*"],
			"@utils/*": ["src/utils/*"],
			"@store/*": ["src/store/*"]
		}
	}
}
